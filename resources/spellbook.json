{
    "content": {
        "spellbook": {
            "proto": "Spellbook",
            "spells": {
                "proto": "OrderedRegistry",
                "list": [
                    "cmake-project",
                    "build.sh",
                    "spell-list",
                    "add-spell",
                    "remove-spell",
                    "add-script-spell",
                    "check-git-upd",
                    "upd-all-build-scripts",
                    "add-cpp-module",
                    "all-bld-upd",
                    "sync",
                    "list",
                    "check-projs",
                    "dm",
                    "lm"
                ],
                "registry": {
                    "cmake-project": {
                        "proto": "Spell",
                        "description": "Create a new CMake project with options: build.sh, Test directory, library, executable",
                        "rules": "copy(templates + \"/CMake/Project\", current_dir + \"/\" + opt_project_name); replace_in_files(current_dir + \"/\" + project_name, \"ProjectName\", opt_project_name);)",
                        "alias": "cmake-project"
                    },
                    "build.sh": {
                        "proto": "Spell",
                        "description": "Add a build.sh script to the current directory",
                        "alias": "build.sh",
                        "options": {
                            "proto": "OrderedRegistry",
                            "list": [
                                "path"
                            ],
                            "registry": {
                                "path": {
                                    "proto": "Option",
                                    "title": "Path",
                                    "description": "File path",
                                    "alias": "path",
                                    "default_value": "."
                                }
                            }
                        }
                    },
                    "add-cpp-module": {
                        "proto": "Spell",
                        "path": "~/Scripts/automation/templates/add_cpp_module.sh",
                        "alias": "add-cpp-module"
                    },
                    "add-spell": {
                        "proto": "Spell",
                        "description": "Create and add a new spell to the Spellbook",
                        "alias": "add-spell",
                        "options": {
                            "proto": "OrderedRegistry",
                            "list": [
                                "alias",
                                "title",
                                "description",
                                "rules",
                                "optoins"
                            ],
                            "registry": {
                                "title": {
                                    "proto": "Option",
                                    "description": "Human-readable short description of a spell to display on screen",
                                    "title": "Title",
                                    "alias": "title"
                                },
                                "description": {
                                    "proto": "Option",
                                    "description": "Human-readable description of a spell to display on screen",
                                    "title": "Description",
                                    "alias": "description"
                                },
                                "rules": {
                                    "proto": "Option",
                                    "description": "A set of spells invocations: a string of expressions of the form <ex>(<a1>, <a2>, …, <an>) divided with semicolon. Supposed to be executed through Python. May be empty if a spell is atomic",
                                    "title": "Rules",
                                    "alias": "rules",
                                    "default_value": ""
                                },
                                "alias": {
                                    "proto": "Option",
                                    "description": "Sequence of chars used to call the spell",
                                    "title": "Alias",
                                    "alias": "alias"
                                }
                            }
                        }
                    },
                    "add-script-spell": {
                        "proto": "Spell",
                        "description": "Add a spell which calls an external script located by a provided path as an option for this spell.",
                        "rules": "",
                        "alias": "add-script-spell",
                        "options": {
                            "proto": "OrderedRegistry",
                            "list": [
                                "alias",
                                "path",
                                "description",
                                "optoins",
                                "rules"
                            ],
                            "registry": {
                                "path": {
                                    "proto": "Option",
                                    "title": "Script path",
                                    "description": "Script path",
                                    "alias": "path"
                                },
                                "alias": {
                                    "proto": "Option",
                                    "title": "Script alias",
                                    "description": "Script alias",
                                    "alias": "alias",
                                    "default_value": ""
                                },
                                "title": {
                                    "proto": "Option",
                                    "description": "Human-readable short description of a spell to display on screen",
                                    "title": "Title",
                                    "alias": "title",
                                    "default_value": ""
                                },
                                "description": {
                                    "proto": "Option",
                                    "description": "Human-readable description of a spell to display on screen",
                                    "title": "Description",
                                    "alias": "description",
                                    "default_value": ""
                                },
                                "rules": {
                                    "proto": "Option",
                                    "description": "A set of spells invocations: a string of expressions of the form <ex>(<a1>, <a2>, …, <an>) divided with semicolon. Supposed to be executed through Python. May be empty if a spell is atomic",
                                    "title": "Rules",
                                    "alias": "rules",
                                    "default_value": ""
                                }
                            }
                        }
                    },
                    "check-git-upd": {
                        "proto": "Spell",
                        "description": "Check git updates script",
                        "rules": "",
                        "alias": "check-git-upd",
                        "path": "~/Scripts/automation/projects/check_git_updates.sh"
                    },
                    "upd-all-build-scripts": {
                        "proto": "Spell",
                        "description": "",
                        "rules": "",
                        "alias": "upd-all-build-scripts",
                        "path": "~/Scripts/automation/all_build_deps/update_build_scripts.sh"
                    },
                    "all-bld-upd": {
                        "proto": "Spell",
                        "path": "~/Scripts/automation/all_build_deps/update_build_scripts.sh",
                        "alias": "all-bld-upd"
                    },
                    "dm": {
                        "proto": "Spell",
                        "description": "This spell switches VSCode and other programs to use dark mode",
                        "title": "Dark mode",
                        "rules": "",
                        "path": "~/Scripts/automation/desktop/switch_light_mode.sh dark",
                        "alias": "dm"
                    },
                    "lm": {
                        "proto": "Spell",
                        "description": "This spell switches VSCode and other programs to use light mode",
                        "title": "Light mode",
                        "rules": "",
                        "path": "~/Scripts/automation/desktop/switch_light_mode.sh light",
                        "alias": "lm"
                    },
                    "sync": {
                        "proto": "Spell",
                        "path": "~/Projects/Spellbook/sync_sb.sh",
                        "alias": "sync"
                    },
                    "list": {
                        "proto": "Spell",
                        "description": "Shows the list of all registered spells",
                        "title": "Spell list",
                        "rules": "-",
                        "alias": "list"
                    },
                    "check-projs": {
                        "proto": "Spell",
                        "path": "~/Scripts/automation/projects/check_git_projects.sh",
                        "alias": "check-projs"
                    }
                }
            }
        },
        "config": {
            "python_spells_directory": "python_spells",
            "shell_spells_directory": "shell_spells",
            "shell_cmd": "bash"
        },
        "script_links": [
            "check-git-upd",
            "upd-all-build-scripts",
            "add-cpp-module",
            "all-bld-upd",
            "sync",
            "check-projs",
            "dm",
            "lm"
        ]
    },
    "types": {
        "OrderedRegistry": {
            "list": [],
            "registry": {}
        },
        "Option": {
            "default_value": "",
            "title": "",
            "description": "",
            "value": "",
            "alias": ""
        },
        "Spellbook": {
            "spells": {
                "proto": "OrderedRegistry"
            }
        },
        "Spell": {
            "rules": "",
            "options": {
                "proto": "OrderedRegistry"
            },
            "description": "",
            "alias": ""
        }
    },
    "private": {}
}