{
    "content": {
        "spellbook": {
            "proto": "Spellbook",
            "spells": {
                "proto": "OrderedRegistry",
                "list": [
                    "cmake-project",
                    "build.sh",
                    "spell-list",
                    "add-spell",
                    "remove-spell",
                    "a",
                    "add-script-spell"
                ],
                "registry": {
                    "build.sh": {
                        "proto": "Spell",
                        "description": "Add a build.sh script to the current directory",
                        "alias": "build.sh",
                        "options": {
                            "proto": "OrderedRegistry",
                            "list": [
                                "path"
                            ],
                            "registry": {
                                "path": {
                                    "proto": "Option",
                                    "description": "File path",
                                    "title": "Path",
                                    "alias": "path",
                                    "default_value": "."
                                }
                            }
                        }
                    },
                    "cmake-project": {
                        "proto": "Spell",
                        "description": "Create a new CMake project with options: build.sh, Test directory, library, executable",
                        "rules": "copy(templates + \"/CMake/Project\", current_dir + \"/\" + opt_project_name); replace_in_files(current_dir + \"/\" + project_name, \"ProjectName\", opt_project_name);)",
                        "alias": "cmake-project"
                    },
                    "a": {
                        "proto": "Spell",
                        "description": "b",
                        "alias": "a",
                        "rules": "c"
                    },
                    "add-script-spell": {
                        "proto": "Spell",
                        "description": "Add a spell which calls an external script located by a provided path as an option for this spell.",
                        "rules": "",
                        "alias": "add-script-spell",
                        "options": {
                            "proto": "OrderedRegistry",
                            "list": [
                                "path",
                                "alias"
                            ],
                            "registry": {
                                "path": {
                                    "proto": "Option",
                                    "description": "Script path",
                                    "title": "Script path",
                                    "alias": "path",
                                    "default_value": ""
                                },
                                "alias": {
                                    "proto": "Option",
                                    "description": "Script alias",
                                    "title": "Script alias",
                                    "alias": "alias",
                                    "default_value": ""
                                }
                            }
                        }
                    },
                    "add-spell": {
                        "proto": "Spell",
                        "description": "Create and add a new spell to the Spellbook",
                        "alias": "add-spell",
                        "options": {
                            "proto": "OrderedRegistry",
                            "list": [
                                "alias",
                                "description",
                                "optoins",
                                "rules"
                            ],
                            "registry": {
                                "description": {
                                    "proto": "Option",
                                    "title": "Description",
                                    "description": "Human-readable description of a spell to display on screen",
                                    "alias": "description"
                                },
                                "title": {
                                    "proto": "Option",
                                    "title": "Title",
                                    "description": "Human-readable short description of a spell to display on screen",
                                    "alias": "title"
                                },
                                "rules": {
                                    "proto": "Option",
                                    "title": "Rules",
                                    "description": "A set of spells invocations: a string of expressions of the form <ex>(<a1>, <a2>, â€¦, <an>) divided with semicolon. Supposed to be executed through Python. May be empty if a spell is atomic",
                                    "alias": "rules"
                                },
                                "alias": {
                                    "proto": "Option",
                                    "title": "Alias",
                                    "description": "Sequence of chars used to call the spell",
                                    "alias": "alias"
                                }
                            }
                        }
                    }
                }
            }
        },
        "config": {
            "python_spells_directory": "python_spells",
            "shell_spells_directory": "shell_spells",
            "shell_cmd": "bash"
        }
    },
    "types": {
        "OrderedRegistry": {
            "list": [],
            "registry": {}
        },
        "Spellbook": {
            "spells": {
                "proto": "OrderedRegistry"
            }
        },
        "Option": {
            "default_value": "",
            "description": "",
            "title": "",
            "value": "",
            "alias": ""
        },
        "Spell": {
            "rules": "",
            "options": {
                "proto": "OrderedRegistry"
            },
            "description": "",
            "alias": ""
        }
    },
    "private": {}
}