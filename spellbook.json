{
    "content": {
        "spellbook": {
            "proto": "Spellbook",
            "spells": {
                "proto": "OrderedRegistry",
                "list": [
                    "cmake-project",
                    "build.sh",
                    "spell-list",
                    "add-spell",
                    "remove-spell"
                ],
                "registry": {
                    "build.sh": {
                        "proto": "Spell",
                        "description": "Add a build.sh script to the current directory",
                        "alias": "build.sh",
                        "options": {
                            "proto": "OrderedRegistry",
                            "list": [
                                "path"
                            ],
                            "registry": {
                                "path": {
                                    "proto": "Option",
                                    "description": "File path",
                                    "title": "Path",
                                    "default_value": ".",
                                    "alias": "path"
                                }
                            }
                        }
                    },
                    "cmake-project": {
                        "proto": "Spell",
                        "description": "Create a new CMake project with options: build.sh, Test directory, library, executable",
                        "alias": "cmake-project",
                        "rules": "copy(templates + \"/CMake/Project\", current_dir + \"/\" + opt_project_name); replace_in_files(current_dir + \"/\" + project_name, \"ProjectName\", opt_project_name);)"
                    },
                    "add-spell": {
                        "proto": "Spell",
                        "description": "Create and add a new spell to the Spellbook",
                        "alias": "add-spell",
                        "options": {
                            "proto": "OrderedRegistry",
                            "list": [
                                "alias",
                                "description",
                                "optoins",
                                "rules"
                            ],
                            "registry": {
                                "alias": {
                                    "proto": "Option",
                                    "title": "Alias",
                                    "description": "Sequence of chars used to call the spell",
                                    "alias": "alias"
                                },
                                "description": {
                                    "proto": "Option",
                                    "title": "Description",
                                    "description": "Human-readable description of a spell to display on screen",
                                    "alias": "description"
                                },
                                "rules": {
                                    "proto": "Option",
                                    "title": "Rules",
                                    "description": "A set of spells invocations: a string of expressions of the form <ex>(<a1>, <a2>, â€¦, <an>) divided with semicolon. Supposed to be executed through Python. May be empty if a spell is atomic",
                                    "alias": "rules"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "types": {
        "Spell": {
            "alias": "",
            "description": "",
            "rules": "",
            "options": {
                "proto": "OrderedRegistry"
            }
        },
        "Spellbook": {
            "spells": {
                "proto": "OrderedRegistry"
            }
        },
        "Option": {
            "alias": "",
            "description": "",
            "title": "",
            "value": "",
            "default_value": ""
        },
        "OrderedRegistry": {
            "registry": {},
            "list": []
        }
    },
    "private": {}
}